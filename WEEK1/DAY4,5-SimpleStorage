 pragma solidity ^0.8.18;

contract SimpleStorage{
    uint myNumber;

   struct Person{   
       string name;
         uint Number;
    }

    Person[] public list;
   function set(uint _number) public virtual{
       myNumber=_number;
    
    }
     /*
      function get() public view returns(uint){
     return myNumber;
     }
    */
    function addPerson(string calldata _name,uint _number) public{
   list.push(Person(_name,_number));
   }
}
